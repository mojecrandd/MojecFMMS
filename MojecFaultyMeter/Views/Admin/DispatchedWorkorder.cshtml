@model IEnumerable<MojecFaultyMeter.Models.DispatchOrders>
@{
    ViewBag.Title = "DispatchedWorkorder";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="main-content">
    <div class="main-content-inner">
        <div class="row">
            <div class="col-lg-12">
                <div class="row">
                    <div class="col-lg-12 mt-5">
                        <div class="card">
                            <div class="card-body">
                                <h4 class="header-title">Dispatch Workorder</h4>
                                <div class="data-tables datatable-primary">
                                    <table id="dataTable3" class="table-responsive-lg">
                                        <thead class="text-capitalize">
                                            <tr>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.DispatchWorkOrder)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.DispatchDate)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Disco)
                                                </th>
                                                <th></th>
                                            </tr>
                                        </thead>
                                        @foreach (var item in Model)
                                        {
                                            <tr>
                                          
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.DispatchWorkOrder)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.DispatchDate)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.Disco)
                                                </td>
                                               
                                            </tr>
                                        }

                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>




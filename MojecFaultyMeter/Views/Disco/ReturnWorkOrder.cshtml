@model IEnumerable<MojecFaultyMeter.Models.FaultyMeters>
@{
    ViewBag.Title = "ReturnWorkOrder";
    Layout = "~/Views/Shared/DiscoLayout.cshtml";
}

<div class="main-content">
    <div class="main-content-inner">
        <div class="row">
            <div class="col-lg-12">
                <div class="row">
                    <div class="col-lg-12 mt-5">
                        <div class="card">
                            <div class="card-body">
                                <h4 class="header-title">Return Workorder</h4>
                                <div class="data-tables datatable-primary">
                                    <table id="dataTable3" class="table-responsive-lg">
                                        <thead class="text-capitalize">
                                            <tr>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.WorkOrderID)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.DiscoUser)
                                                </th>                              
                                                <th></th>
                                            </tr>
                                        </thead>
                                        @foreach (var item in Model)
                                        {
                                            <tr>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.WorkOrderID)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.DiscoUser)
                                                </td>                                          
                                                <td>
                                                    @Html.ActionLink("Details", "GetOrderDetails", new { id = item.WorkOrderID }, new { @class = "btn btn-primary" })
                                                </td>
                                            </tr>
                                        }

                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

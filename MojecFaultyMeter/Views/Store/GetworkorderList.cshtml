@model IEnumerable<MojecFaultyMeter.Models.FaultyMeters>
@{
    ViewBag.Title = "GetworkorderList";
    Layout = "~/Views/Shared/_StoreLayout.cshtml";
}

<div class="main-content">
    <div class="main-content-inner">
        <div class="row">
            <div class="col-lg-12">
                <div class="row">
                    <div class="col-lg-12 mt-5">
                        <div class="card">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-md-12 pl-6">
                                        <div class="form-group">
                                            <a href="@Url.Action("Workorders", "Store")" class="btn btn-outline-primary">
                                                <i class="fas fa-plus-square"></i>
                                                <span>
                                                    <strong>Back to List</strong>
                                                </span>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <h4 class="header-title">Work Order Details</h4>
                                <div class="data-tables datatable-primary">
                                    <table id="dataTable3" class="table-responsive-lg">
                                        <thead class="text-capitalize">
                                            <tr>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.MeterNo)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.CustomerName)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.AccountNo)
                                                </th>

                                                <th>
                                                    @Html.DisplayNameFor(model => model.MeterType)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Status)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Replacementstat)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.ReturnDate)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.AcceptedBy)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.TreatedBy)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.DiscoUser)
                                                </th>

                                            </tr>
                                        </thead>
                                        @foreach (var item in Model)
                                        {
                                            <tr>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.MeterNo)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.CustomerName)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.AccountNo)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.MeterType)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.Status)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.Replacementstat)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.ReturnDate)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.AcceptedBy)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.TreatedBy)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.DiscoUser)
                                                </td>

                                            </tr>
                                        }

                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>





